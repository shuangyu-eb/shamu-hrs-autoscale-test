server:
  tomcat:
    protocol_header: x-forwarded-proto
    remote_ip_header: x-forwarded-for
  use-forward-headers: true
  port: 5000

cloud:
  aws:
    region:
      auto: true

cors:
  allowedOrigins:
    - {{website_end_point}}
    - https://{{auth0_domain}}


sentry:
  enable: true
  env: {{env}}
  release: mvp
