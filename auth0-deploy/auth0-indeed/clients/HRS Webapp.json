{
  "is_token_endpoint_ip_header_trusted": false,
  "name": "HRS Webapp",
  "is_first_party": true,
  "oidc_conformant": false,
  "sso_disabled": false,
  "cross_origin_auth": false,
  "allowed_clients": [],
  "allowed_logout_urls": [
    "##ENVIRONMENT_URL##"
  ],
  "callbacks": [
    "##ENVIRONMENT_URL##/parse"
  ],
  "description": "Indeed HRIS environment",
  "native_social_login": {
    "apple": {
      "enabled": false
    },
    "facebook": {
      "enabled": false
    }
  },
  "allowed_origins": [
    "##ENVIRONMENT_URL##"
  ],
  "jwt_configuration": {
    "alg": "RS256",
    "lifetime_in_seconds": 43200,
    "secret_encoded": false
  },
  "client_aliases": [],
  "token_endpoint_auth_method": "none",
  "app_type": "spa",
  "grant_types": [
    "authorization_code",
    "implicit",
    "refresh_token"
  ],
  "web_origins": [
    "##ENVIRONMENT_URL##"
  ],
  "custom_login_page_on": true
}
